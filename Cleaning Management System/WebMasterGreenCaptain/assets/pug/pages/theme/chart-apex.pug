                              h5 Mixed Chart
                            .card-body
                              #mixedchart
                        .col-sm-12.col-xl-6.box-col-6
                          .card
                            .card-header.pb-0
                              h5
                                | Radar Chart 
                            .card-body
                              #radarchart
                        .col-sm-12.col-xl-6.box-col-6
                          .card
                            .card-header.pb-0
                              h5
                                | Radial Bar Chart
                            .card-body
                              #circlechart
                    // Container-fluid Ends
                include ../../components/footer
        include ../../components/footer-files