- var tree                  = true;
- var theme_customizer      = true;

doctype html
html(lang='en')
    include ../../components/header-files
    body
        include ../../components/loader
        // page-wrapper Start
        #pageWrapper.page-wrapper
            include ../../components/header
            // Page Body Start
            .page-body-wrapper.horizontal-menu
                include ../../components/sidebar
                .page-body
                    .container-fluid
                        .page-header
                            .row
                                .col-sm-6
                                    h3
                                      | Tree View
                                    ol.breadcrumb
                                      li.breadcrumb-item
                                        a(href='index.html')
                                         | Home
                                      li.breadcrumb-item Advance
                                      li.breadcrumb-item.active Tree View
                                .col-sm-6
                                  include ../../components/bookmark
                    // Container-fluid starts
                    .container-fluid
                      .row
                        .col-sm-6
                          .card
                            .card-header.pb-0
                              h5 Basic Tree
                            .card-body
                              #treeBasic
                                ul
                                  li
                                    | Admin
                                    ul
                                      li(data-jstree='{"opened":true}')
                                        | Assets
                                        ul
                                          li(data-jstree='{"opened":false}')
                                            | Css
                                            ul
                                              li(data-jstree='{"selected":false,"type":"file"}') Css one
                                              li(data-jstree='{"type":"file"}') Css two
                                          li(data-jstree='{"opened":true}')
                                            | Js
                                            ul
                                              li(data-jstree='{"selected":true,"type":"file"}') Js one
                                              li(data-jstree='{"type":"file"}') Js two
                                          li(data-jstree='{"opened":true}')
                                            | Scss
                                            ul
                                              li(data-jstree='{"opened":false}')
                                                | Sub Child
                                                ul
                                                  li(data-jstree='{"selected":false,"type":"file"}') Sub File
                                                  li(data-jstree='{"type":"file"}') Sub File
                                              li(data-jstree='{"type":"file"}') Scss two
                                      li(data-jstree='{"opened":true}')
                                        | Default
                                        ul
                                          li(data-jstree='{"type":"file"}') Dashboard
                                          li(data-jstree='{"type":"file"}') Typography
                                  li(data-jstree='{"type":"file"}') index file
                        .col-sm-6
                          .card
                            .card-header.pb-0
                              h5 Checkbox Tree
